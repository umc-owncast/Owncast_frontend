<?xml version="1.0" encoding="utf-8"?>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:id="@+id/key_fin_dialog_cl"
        android:layout_width="340dp"
        android:layout_height="268dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@drawable/keyword_aud_save_finish_dialog_bg">

        <ImageView
            android:id="@+id/key_fin_dialog_play_iv"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_play_btn"
            android:layout_marginTop="10dp"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/key_fin_cardview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="20dp"
        android:layout_marginTop="38dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="@id/key_fin_dialog_play_iv"
        app:layout_constraintTop_toBottomOf="@id/key_fin_dialog_play_iv"
        android:background="@drawable/keyword_aud_save_src_bg"
        app:cardElevation="0dp">
        <ImageView
            android:id="@+id/key_fin_cast_img_iv"
            android:layout_width="108dp"
            android:layout_height="108dp"
            android:src="@drawable/save_keyword_thumb_ex1"

            android:scaleType="centerCrop"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

    </androidx.cardview.widget.CardView>


        <TextView
            android:id="@+id/key_fin_gohear_tv"
            android:layout_width="199dp"
            android:layout_height="40dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/key_aud_scr_btn_bg"
            android:backgroundTint="@color/main_color"
            android:gravity="center"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="3sp"
            android:text="바로 들으러 가기"
            android:textColor="@color/white"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/key_fin_cast_time_tv"
            android:layout_width="41dp"
            android:layout_height="16dp"
            android:layout_marginStart="4dp"
            android:background="@drawable/keyword_act_favor_key_bg"
            android:backgroundTint="#00051F"
            android:gravity="center"
            android:letterSpacing="0.05"
            android:lineSpacingExtra="3sp"
            android:text="4:18"
            android:textColor="@color/white"
            android:textSize="11sp"
            android:translationY="-1.44sp"
            app:layout_constraintStart_toEndOf="@id/key_fin_cast_title_tv"
            app:layout_constraintTop_toTopOf="@id/key_fin_cast_title_tv"
            app:layout_constraintBottom_toBottomOf="@id/key_fin_cast_title_tv" />

        <TextView
            android:id="@+id/key_fin_cast_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginStart="8dp"
            android:gravity="center"
            android:lineSpacingExtra="-4sp"
            android:text="캐스트 제목"
            android:textColor="#00051F"
            android:textSize="20sp"
            android:textStyle="bold"
            android:translationY="-1.44sp"
            app:layout_constraintStart_toEndOf="@id/key_fin_cardview"
            app:layout_constraintTop_toTopOf="@id/key_fin_cardview" />

        <TextView
            android:id="@+id/key_fin_cast_category_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:gravity="center"
            android:lineSpacingExtra="-2sp"
            android:letterSpacing="0.01"
            android:text="카테고리 6"
            android:textColor="#00051F"
            android:textSize="15sp"
            android:translationY="-1.44sp"
            app:layout_constraintStart_toStartOf="@id/key_fin_cast_title_tv"
            app:layout_constraintTop_toBottomOf="@id/key_fin_cast_title_tv" />

        <TextView
            android:id="@+id/key_fin_scrpit_1st_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:layout_marginBottom="9dp"
            android:layout_marginStart="8dp"
            android:gravity="center"
            android:text="(닉네임)님만의"
            android:textColor="#00051F"
            android:textSize="15sp"
            app:layout_constraintStart_toEndOf="@id/key_fin_dialog_play_iv"
            app:layout_constraintTop_toTopOf="@id/key_fin_dialog_play_iv"
            app:layout_constraintBottom_toBottomOf="@id/key_fin_dialog_play_iv" />

        <ImageView
            android:id="@+id/key_fin_owncast_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:src="@drawable/owncast_ic"
            app:layout_constraintStart_toEndOf="@id/key_fin_scrpit_1st_tv"
            app:layout_constraintTop_toTopOf="@id/key_fin_scrpit_1st_tv"
            app:layout_constraintBottom_toBottomOf="@id/key_fin_scrpit_1st_tv" />

        <TextView
            android:id="@+id/key_fin_scrpit_2nd_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:layout_marginBottom="9dp"
            android:layout_marginStart="2dp"
            android:gravity="center"
            android:text="가 만들어 졌어요!"
            android:textColor="#00051F"
            android:textSize="15sp"
            app:layout_constraintStart_toEndOf="@id/key_fin_owncast_iv"
            app:layout_constraintTop_toTopOf="@id/key_fin_dialog_play_iv"
            app:layout_constraintBottom_toBottomOf="@id/key_fin_dialog_play_iv" />

        <TextView
            android:id="@+id/key_fin_home_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:gravity="center"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="3sp"
            android:text="홈으로"
            android:textColor="#00051F"
            android:textSize="14sp"
            android:background="@drawable/ui_btn_bg"
            app:layout_constraintStart_toStartOf="@id/key_fin_cardview"
            app:layout_constraintBottom_toBottomOf="@id/key_fin_gohear_tv" />
    </androidx.constraintlayout.widget.ConstraintLayout>












<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_centerHorizontal="true"
    android:layout_centerVertical="true"
    android:background="@android:color/transparent"
    >
    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/particle_animation_av"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:lottie_rawRes="@raw/particle_animation"
        app:lottie_autoPlay="true"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"



        />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/key_fin_dialog_cl"
        android:layout_width="340dp"
        android:layout_height="268dp"

        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="200dp"

        android:background="@drawable/keyword_audio_out_dialog_bg">
        <ImageView
            android:id="@+id/key_fin_dialog_play_iv"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_play_btn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="10dp"
            android:layout_marginStart="8dp"/>

        <ImageView


            android:id="@+id/key_fin_cast_img_iv"
            android:layout_width="108dp"
            android:layout_height="108dp"
            android:src="@drawable/keyword_aud_save_src_bg"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toStartOf="@id/key_fin_dialog_play_iv"

            app:layout_constraintTop_toBottomOf="@id/key_fin_dialog_play_iv"
            android:layout_marginTop="38dp"/>
        <TextView
            android:id="@+id/key_fin_gohear_tv"
            android:textColor="@color/white"
            android:textSize="14sp"
            android:text="바로 들으러 가기"
            android:layout_width="199dp"

            android:layout_height="40dp"
            android:backgroundTint="@color/main_color"
            android:gravity="center"
            android:background="@drawable/key_aud_scr_btn_bg"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="10dp"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="3sp"
            android:layout_marginTop="16dp"/>
        <TextView
            android:id="@+id/key_fin_cast_time_tv"
            android:layout_width="41dp"
            android:layout_height="16dp"
            android:background="@drawable/keyword_act_favor_key_bg"
            android:backgroundTint="#00051F"
            android:text="4:18"
            android:lineSpacingExtra="3sp"
            android:translationY="-1.44sp"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="11sp"
            android:letterSpacing="0.05"
            android:layout_marginStart="4dp"
            app:layout_constraintTop_toTopOf="@id/key_fin_cast_title_tv"
            app:layout_constraintBottom_toBottomOf="@id/key_fin_cast_title_tv"
            app:layout_constraintStart_toEndOf="@id/key_fin_cast_title_tv"/>
        <TextView
            android:id="@+id/key_fin_cast_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="캐스트 제목"
            android:textStyle="bold"
            android:lineSpacingExtra="-4sp"
            android:translationY="-1.44sp"
            android:gravity="center"
            android:textColor="#00051F"
            android:textSize="20sp"
            app:layout_constraintStart_toEndOf="@id/key_fin_cast_img_iv"
            app:layout_constraintTop_toTopOf="@id/key_fin_cast_img_iv"
            android:layout_marginTop="10dp"
            android:layout_marginStart="8dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="카테고리 6"
            android:lineSpacingExtra="-2sp"
            android:translationY="-1.44sp"
            android:gravity="center"
            android:textColor="#00051F"
            android:textSize="15sp"
            android:letterSpacing="0.01"
            app:layout_constraintTop_toBottomOf="@id/key_fin_cast_title_tv"
            app:layout_constraintStart_toStartOf="@id/key_fin_cast_title_tv"/>
        <TextView
            android:id="@+id/key_fin_scrpit_1st_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="(닉네임)님만의"
            android:textSize="15sp"
            android:gravity="center"
            android:textColor="#00051F"
            app:layout_constraintStart_toEndOf="@id/key_fin_dialog_play_iv"
            app:layout_constraintTop_toTopOf="@id/key_fin_dialog_play_iv"
            app:layout_constraintBottom_toBottomOf="@id/key_fin_dialog_play_iv"
            android:layout_marginTop="3dp"
            android:layout_marginBottom="9dp"
            android:layout_marginStart="8dp"
            />
        <ImageView
            android:id="@+id/key_fin_owncast_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/owncast_ic"
            app:layout_constraintStart_toEndOf="@id/key_fin_scrpit_1st_tv"
            app:layout_constraintTop_toTopOf="@id/key_fin_scrpit_1st_tv"
            app:layout_constraintBottom_toBottomOf="@id/key_fin_scrpit_1st_tv"
            android:layout_marginStart="2dp"/>
        <TextView
            android:id="@+id/key_fin_scrpit_2nd_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="가 만들어 졌어요!"
            android:textSize="15sp"
            android:gravity="center"
            android:textColor="#00051F"
            app:layout_constraintStart_toEndOf="@id/key_fin_owncast_iv"
            app:layout_constraintTop_toTopOf="@id/key_fin_dialog_play_iv"
            app:layout_constraintBottom_toBottomOf="@id/key_fin_dialog_play_iv"
            android:layout_marginTop="3dp"
            android:layout_marginBottom="9dp"
            android:layout_marginStart="2dp"
            />
        <TextView
            android:id="@+id/key_fin_home_tv"
            android:textColor="#00051F"
            android:textSize="14sp"
            android:text="홈으로"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"

            android:gravity="center"
            android:background="@drawable/ui_btn_bg"
            app:layout_constraintStart_toStartOf="@id/key_fin_cast_img_iv"
            app:layout_constraintBottom_toBottomOf="@id/key_fin_gohear_tv"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="3sp"
            android:layout_marginTop="16dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>








</androidx.constraintlayout.widget.ConstraintLayout>